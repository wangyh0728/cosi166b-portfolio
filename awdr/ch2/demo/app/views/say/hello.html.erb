<h1>Hello From Rails!</h1>
<p>
	It is now <%= @time %>
	Say <a href="/say/goodbye">Goodbye</a>!
</p>
<p>
	Time to say
	<%= link_to "Goodbye", say_goodbye_path %>!
</p>
<p>
	<%= 1+2 %>
</p>
<p>
	<%= 0.5.hour.from_now.localtime %>
</p>
<p>
	<%= "cow" + "boy" %>
</p>
<p>
	<%= @files = Dir.glob('*') %>
</p>
<p>
	<% for file in @files %>
	<p>file name is: <%= file %></p>
	<% end %>
</p>

